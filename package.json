{
  "name": "nycticorax",
  "version": "4.0.1",
  "description": "State container for JavaScript application, and React",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noemit && eslint . --ext .ts,.tsx,.js",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test": "npm run lint && npm run test:coverage",
    "start": "webpack serve --progress",
    "build": "NODE_ENV=production webpack --progress",
    "docs": "rm -rf docs && NODE_ENV=production webpack --progress",
    "esm": "tsc --p tsconfig-esm.json",
    "cjs": "tsc --p tsconfig-cjs.json",
    "build0": "npm run docs && rm -rf dist && npm run esm && npm run cjs && npm run umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fratercula/nycticorax.git"
  },
  "keywords": [
    "store",
    "state",
    "react"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "bugs": {
    "url": "https://github.com/fratercula/nycticorax/issues"
  },
  "homepage": "https://github.com/fratercula/nycticorax#readme",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^26.0.24",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-loader": "^8.2.5",
    "enzyme": "^3.11.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.5",
    "typescript": "^4.4.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "core-js": "^3.24.1",
    "fast-deep-equal": "^3.1.3",
    "lodash.clonedeep": "^4.5.0"
  }
}
